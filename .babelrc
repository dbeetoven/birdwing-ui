{
  "presets": ["@babel/preset-env", "@babel/preset-react"],
  "plugins": [
    [
      "babel-plugin-styled-components",
      {
        "displayName": false,
        "pure": true
      }
    ],
    ["module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "test": "./test",
          "underscore": "lodash",
          "assets": "./src/assets/",
          "config": "./src/config/",
          "services": "./src/services/",
          "hooks": "./src/hooks/",
          "styles": "./src/styles/",
          "themes": "./src/themes/",
          "utils": "./src/utils/",
          "pages": "./src/pages/",
          "contexts": "./src/contexts/",
          "components": "./src/components/"
        }
      }]
  ]
}
